{
  "env": {
    "es": {
      "presets": [[ "@babel/env", { "modules": false } ]]
    },

    "cjs": {
      "presets": [[ "@babel/env" ]],
      "plugins": [
        "add-module-exports"
      ]
    },

    "node": {
      "presets": [[ "@babel/env", { "targets": { "node": 6 } } ]],
      "plugins": [
        "add-module-exports",
        ["module-resolver", {
          "alias": {
            "lowline": "lodash",
            "mini-qs": "querystring"
          }
        }]
      ]
    }
  }
}
