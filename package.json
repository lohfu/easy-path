{
  "name": "easy-path",
  "version": "0.1.0",
  "description": "General purpose front end URL router",
  "main": "node/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rimraf node",
    "lint": "eslint src",
    "build": "npm run build:cjs",
    "build:cjs": "BABEL_ENV=node babel src --out-dir node",
    "test": "BABEL_ENV=node node tests/index.js | tap-spec",
    "prepublish": "npm run lint && npm run clean && npm run build",
    "preversion": "npm run lint"
  },
  "keywords": [
    "router"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "contributors": [
    "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lohfu/easy-path.git"
  },
  "bugs": {
    "url": "https://github.com/lohfu/easy-path/issues"
  },
  "homepage": "https://github.com/lohfu/easy-path",
  "dependencies": {
    "lodash": "^4.17.4",
    "lowline": "^0.1.3",
    "mini-qs": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-es2015-node6": "^0.4.0",
    "babel-register": "^6.24.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
