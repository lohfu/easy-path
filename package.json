{
  "name": "nooks",
  "version": "0.1.2",
  "description": "General purpose front end URL router",
  "main": "node/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "browser": {
    "node/index.js": "cjs/index.js"
  },
  "scripts": {
    "clean": "rimraf cjs es node",
    "lint": "eslint src",
    "build": "npm run build:cjs && npm run build:es && npm run build:node",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir cjs",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:node": "BABEL_ENV=node babel src --out-dir node",
    "test": "BABEL_ENV=node node tests/index.js | tap-spec",
    "prepublish": "npm run lint && npm run clean && npm run build",
    "preversion": "npm run lint"
  },
  "keywords": [
    "router"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "contributors": [
    "Linus Miller <lohfu@lohfu.io> (https://lohfu.io/)"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lohfu/nooks.git"
  },
  "bugs": {
    "url": "https://github.com/lohfu/nooks/issues"
  },
  "homepage": "https://github.com/lohfu/nooks",
  "dependencies": {
    "lowline": "^0.1.6",
    "mini-qs": "0.0.1"
  },
  "optionalDependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.35",
    "@babel/core": "^7.0.0-beta.35",
    "@babel/preset-env": "^7.0.0-beta.35",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-module-resolver": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  }
}
